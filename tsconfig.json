{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "jsx": "react-jsx",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "resources/js",
      "paths": {
        "@/*": ["*"]
      }
    },
    "include": ["resources/js/**/*.ts", "resources/js/**/*.tsx"],
    "exclude": ["node_modules"]
  }
  